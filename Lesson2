 var todos = ['item 1', 'item 2', 'item 3']
 
 function displayTodos() {
 console.log('My todos:', todos);
} 
displayTodos()
 My todos: ["item 1", "item 2", "item 3"]
 
 function addTodo(){
  todos.push('new todo');
 }
 
 displayTodos()
 My todos: ["item 1", "item 2", "item 3", "item 4"]
 
 function addTodo(){
  todos.push('new todo');
  displayTodos();
 }
 
 addTodo()
 My todos: ["item 1", "item 2", "item 3", "new todo", "new todo", "new todo"]
 
